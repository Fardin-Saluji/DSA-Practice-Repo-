// Problem: Write a program to find the sum of the prime factors of a given number.
// Input: number = 12
// Output: 5

function sumOfPrimeFactors(number) {
  let sum = 0;
  let factor = 2;

  
  while (number > 1) {
    if (number % factor === 0) {
      sum += factor; 
      while (number % factor === 0) {
        number /= factor;
      }
    }
    factor++;
  }

  return sum;
}

const number = 12;
console.log(sumOfPrimeFactors(number)); 
