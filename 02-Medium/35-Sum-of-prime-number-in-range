// Problem: Write a program to calculate the sum of all prime numbers within a given range
// Input: range = [1, 10]
// Output: 17

function isPrime(n) {
    if (n < 2) return false;
    for (let i = 2; i <= Math.sqrt(n); i++) {
        if (n % i === 0) return false;
    }
    return true;
}

function sumOfPrimesInRange(start, end) {
    let sum = 0;
    for (let i = start; i <= end; i++) {
        if (isPrime(i)) {
            sum += i;
        }
    }
    return sum;
}

const rangeStart = 1;
const rangeEnd = 10;
