// Problem: Write a program to find the largest element in each row of a matrix
// Input: matrix = [[1, 2, 3], [4, 5, 6], [7, 8, 9]]
// Output: [3, 6, 9]

function largestInRows(matrix) {
  return matrix.map(row => Math.max(...row));
}


const matrix = [
  [1, 2, 3],
  [4, 5, 6],
  [7, 8, 9]
];

console.log(largestInRows(matrix)); 
