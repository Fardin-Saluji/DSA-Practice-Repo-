// Problem: Write a program to find the sum of all even numbers within a given range
// Input: range = [1, 10]
// Output: 30

function sumOfEvenNumbers(start, end) {
    let sum = 0;
    for (let i = start; i <= end; i++) {
        if (i % 2 === 0) {
            sum += i;
        }
    }
    return sum;
}

const rangeStart = 1;
const rangeEnd = 10;
const result = sumOfEvenNumbers(rangeStart, rangeEnd);
console.log("Sum of even numbers:", result);
