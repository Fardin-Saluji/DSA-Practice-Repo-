// Problem: Write a program to create a pattern where numbers increase with each row
// Input: rows = 3
// Output: 
// 1
// 12
// 123

function generatePattern(rows) {
    for (let i = 1; i <= rows; i++) {
        let pattern = "";
        for (let j = 1; j <= i; j++) {
            pattern += j;
        }
        console.log(pattern);
    }
}

generatePattern(3);
