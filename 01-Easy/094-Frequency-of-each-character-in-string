// Problem: Write a program to count the frequency of each character in a given string
// Input: string = "hello"
// Output: {'h': 1, 'e': 1, 'l': 2, 'o': 1,}

function charFrequency(str) {
  const freq = {};

  for (let char of str) {
    freq[char] = (freq[char] || 0) + 1;
  }

  return freq;
}


const string = "hello";
console.log(charFrequency(string)); 
